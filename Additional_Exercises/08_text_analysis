,"      # stemming\n","      text_to_clean = snowStem.stem(text_to_clean)\n","    \n","    if remove_stopwords:\n","        # remove stop words\n","        text_to_clean = ' '.join([w for w in text_to_clean.split() if not(w in STOPWORDS)])\n","    \n","    return text_to_clean"],"metadata":{"id":"-3hMMLcqESw-","executionInfo":{"status":"ok","timestamp":1642592885486,"user_tz":-60,"elapsed":2,"user":{"displayName":"Lukas Marx","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"12089797695045899740"}}},"execution_count":74,"outputs":[]},{"cell_type":"markdown","source":["So to start of we create a new colum that contains the preprocessed texts."],"metadata":{"id":"4kWdmUrhEnH7"}},{"cell_type":"code","source":["df.loc[:, 'clean_texts'] = [my_preprocessor(text, remove_stopwords = True) for text in df.content]"],"metadata":{"id":"83KhMjcCEY8J","executionInfo":{"status":"ok","timestamp":1642592911987,"user_tz":-60,"elapsed":26503,"user":{"displayName":"Lukas Marx","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"12089797695045899740"}}},"execution_count":75,"outputs":[]},{"cell_type":"code","source":["df"],"metadata":{"id":"kb5tUH5tEkvW","colab":{"base_uri":"https://localhost:8080/","height":424},"executionInfo":{"status":"ok","timestamp":1642592911989,"user_tz":-60,"elapsed":24,"user":{"displayName":"Lukas Marx","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"12089797695045899740"}},"outputId":"7da987f4-9373-4c03-906f-5899029b9e60"},"execution_count":76,"outputs":[{"output_type":"execute_result","data":{"text/html":["\n","  <div id=\"df-a0ce6a14-4215-4a7e-bfa3-b7b749809272\">\n","    <div class=\"colab-df-container\">\n","      <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n"," 